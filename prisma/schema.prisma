generator client {
    provider = "prisma-client-js"
    output   = "generated"
}

datasource db {
    provider = "postgresql"
    url      = env("DATABASE_URL")
}

model User {
    id        String   @id @default(cuid())
    createdAt DateTime @default(now()) @map("created_at")
    updatedAt DateTime @updatedAt @map("updated_at")

    email    String  @unique
    username String?
    password String

    @@map("user")
}

